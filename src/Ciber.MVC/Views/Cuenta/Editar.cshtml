@model Ciber.core.Cuenta

@{
    ViewData["Title"] = "Editar Cuenta";
}

<h2>@ViewData["Title"]</h2>

<form asp-action="Editar">
    <div class="form-group">
        <label asp-for="Ncuenta"></label>
        <input asp-for="Ncuenta" class="form-control" readonly />
    </div>

    <div class="form-group">
        <label asp-for="Nombre"></label>
        <input asp-for="Nombre" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="HoraRegistrada"></label>
        <input asp-for="HoraRegistrada" class="form-control" readonly />
    </div>

    <button type="submit" class="btn btn-success">Guardar cambios</button>
    <a asp-action="Index" class="btn btn-secondary">Cancelar</a>
</form>

